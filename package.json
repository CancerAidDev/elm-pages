{
  "name": "elm-pages",
  "version": "1.5.5",
  "homepage": "http://elm-pages.com",
  "moduleResolution": "node",
  "description": "Type-safe static sites, written in pure elm with your own custom elm-markup syntax.",
  "main": "index.js",
  "scripts": {
    "start": "cd examples/docs && npm start",
